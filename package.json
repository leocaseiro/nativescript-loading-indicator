
{
  "name": "nativescript-loading-indicator-new",
  "version": "2.1.0",
  "description": "A NativeScript plugin for showing an overlayed loading indicator.",
  "repository": {
    "type": "git",
    "url": "https://github.com/pocketsmith/nativescript-loading-indicator-new.git"
  },
  "main": "loading-indicator.js",
  "typings": "index.d.ts",
  "author": {
    "name": "PocketSmith"
  },
  "contributors": [
    {
      "name": "Nathan Walker <NathanWalker>",
      "email": "walkerrunpdx@gmail.com"
    },
    {
      "name": "Leo Caseiro <leocaseiro>",
      "email": "leobok@gmail.com"
    }
  ],
  "keywords": [
    "NativeScript",
    "loading-indicator",
    "Loading",
    "Progress",
    "MBProgressHUD",
    "ProgressDialog",
    "Android",
    "iOS"
  ],
  "nativescript": {
    "platforms": {
      "android": "2.1.1",
      "ios": "2.1.1"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo; cd demo; tns emulate ios",
    "demo.android": "npm run preparedemo; cd demo; tns run android",
    "preparedemo": "npm run build; cd demo; tns plugin remove nativescript-loading-indicator-new; tns plugin add ..; tns install",
    "setup": "cd demo; npm install; cd ..; npm run build; cd demo; tns plugin add ..; cd .."
  },
  "license": {
